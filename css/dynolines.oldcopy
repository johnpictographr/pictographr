.dynoline .line.rightArrow:after{
width: 0px !important;
height: 0px !important;
right: 99%;
border-top: 6px solid transparent;
border-bottom: 6px solid transparent; 
border-right:10px solid; 
top:-6px;
border-right-color: inherit;
}
.dynoline .line.leftArrow:before{
content: url(../img/arrow.svg);
width: 12px;
right:-10px;
top:-11px;
position: absolute;
-webkit-transform: rotate(180deg); 
}




.dynoline .line.thicker.rightArrow:after{
border-top: 18px solid transparent;
border-bottom: 17px solid transparent;
border-right: 28px solid;
top: -11px;
}
.dynoline .line.thicker.leftArrow:before{
border-top: 18px solid transparent;
border-bottom: 17px solid transparent;
border-left: 28px solid;
top: -11px;
}



.dynoline .line.thickest.rightArrow:after{
border-top: 31px solid transparent;
border-bottom: 31px solid transparent;
border-right: 48px solid;
top: -17px
}
.dynoline .line.thickest.leftArrow:before{
border-top: 31px solid transparent;
border-bottom: 31px solid transparent;
border-left: 48px solid;
top: -16px;
}


.dynoline .line.rightCircle:after{
right: 100%;
border:2px solid;
border-radius: 50%;
display: inline-block;
border-color: inherit;
}
.dynoline .line.leftCircle:before{
left: 100%;
border:2px solid;
border-radius: 50%;
display: inline-block;
border-color: inherit;
}

.dynoline .line.size30:after,
.dynoline .line.size30:before
{
width: 30px;
height: 30px;
top:-17px;
}
.dynoline .line.size20:after,
.dynoline .line.size20:before{
width: 20px;
height: 20px;
top: -10px;
}
.dynoline .line.size10:after,
.dynoline .line.size10:before{
width: 10px;
height: 10px;
top: -5px;
}
